{
  "name": "webpacktest-devserver",
  "version": "1.0.0",
  "description": "webpack testing",
  "main": "public/index.html",
  "author": "kroko",
  "license": "MIT",
  "private": true,
  "config": {
    "portFrontendMainHTTP": 80,
    "portFrontendMainHTTPS": 443,
    "portFrontendAppHTTP": 3000,
    "portFrontendAppHTTPS": 3001,
    "portFrontendWebpackDevServerHTTP": 4000,
    "portFrontendWebpackDevServerHTTPS": 4001,
    "isWebpackDevServerHTTPS": false,
    "isWebpackDevServerHot": true,
    "hostDevelopment": "webpacktest-devserver.test",
    "pathAboveRootDevelopment": "",
    "hostTesting": "webpacktest-devserver.test",
    "pathAboveRootTesting": "",
    "hostStaging": "webpacktest-devserver.test",
    "pathAboveRootStaging": "",
    "hostProduction": "webpacktest-devserver.test",
    "pathAboveRootProduction": ""
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:front:dev": "npm run clean:assets && NODE_ENV=development webpack-dev-server --config=$(pwd)/webpack.front.config.js -d",
    "build:front:test": "npm run clean:assets && NODE_ENV=testing webpack --config=$(pwd)/webpack.front.config.js --progress",
    "build:front:stage": "npm run clean:assets && NODE_ENV=staging webpack --config=$(pwd)/webpack.front.config.js --progress",
    "build:front:prod": "npm run clean:assets && NODE_ENV=production webpack --config=$(pwd)/webpack.front.config.js --progress",
    "clean:assets": "rm -rf $(pwd)/public/assets/*",
    "screen:start": "npm run screen:stop && screen -S $npm_package_name -d -m npm run build:front:dev",
    "screen:enter": "screen -r $npm_package_name",
    "screen:stop": "screen -S $npm_package_name -X quit 2>/dev/null || :",
    "fly:front:testing": "echo \"Flightplan is out of sope for this tut\" && exit 0",
    "fly:front:staging": "echo \"Flightplan is out of sope for this tut\" && exit 0",
    "fly:front:production": "echo \"Flightplan is out of sope for this tut\" && exit 0"
  },
  "devDependencies": {
    "autoprefixer": "^8.4.1",
    "css-loader": "^0.28.11",
    "css-mqpacker": "^6.0.2",
    "cssnano": "^3.10.0",
    "file-loader": "^1.1.11",
    "filemanager-webpack-plugin": "^1.0.26",
    "image-webpack-loader": "^4.2.0",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.9.0",
    "normalize.css": "^7.0.0",
    "postcss-loader": "^2.1.5",
    "resolve-url-loader": "github:bholloway/resolve-url-loader",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "url-loader": "^1.0.1",
    "webpack": "^4.7.0",
    "webpack-cli": "^2.1.2",
    "webpack-dev-server": "^3.1.4"
  }
}
