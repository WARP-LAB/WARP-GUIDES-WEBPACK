{
  "name": "webpacktest-codesplitting",
  "version": "1.0.0",
  "description": "webpack testing",
  "main": "public/index.html",
  "author": "kroko",
  "license": "MIT",
  "private": true,
  "config": {
    "portFrontendMainHTTP": 80,
    "portFrontendMainHTTPS": 443,
    "portFrontendAppHTTP": 3000,
    "portFrontendAppHTTPS": 3001,
    "portFrontendWebpackDevServerHTTP": 4000,
    "portFrontendWebpackDevServerHTTPS": 4001,
    "isWebpackDevServerHTTPS": false,
    "isWebpackDevServerHot": true,
    "hostDevelopment": "webpacktest-codesplitting.test",
    "pathAboveRootDevelopment": "",
    "hostTesting": "webpacktest-codesplitting.test",
    "pathAboveRootTesting": "",
    "hostStaging": "webpacktest-codesplitting.test",
    "pathAboveRootStaging": "",
    "hostProduction": "webpacktest-codesplitting.test",
    "pathAboveRootProduction": ""
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:front:dev": "npm run clean:assets && NODE_ENV=development webpack-dev-server --config=$(pwd)/webpack.front.config.js -d",
    "build:front:test": "npm run clean:assets && NODE_ENV=testing webpack --config=$(pwd)/webpack.front.config.js --progress",
    "build:front:stage": "npm run clean:assets && NODE_ENV=staging webpack --config=$(pwd)/webpack.front.config.js --progress",
    "build:front:prod": "npm run clean:assets && NODE_ENV=production webpack --config=$(pwd)/webpack.front.config.js --progress",
    "clean:assets": "rm -rf $(pwd)/public/assets/* && rm -rf $(pwd)/public/index.html",
    "screen:start": "npm run screen:stop && screen -S $npm_package_name -d -m npm run build:front:dev",
    "screen:enter": "screen -r $npm_package_name",
    "screen:stop": "screen -S $npm_package_name -X quit 2>/dev/null || :",
    "fly:front:testing": "echo \"Flightplan is out of sope for this tut\" && exit 0",
    "fly:front:staging": "echo \"Flightplan is out of sope for this tut\" && exit 0",
    "fly:front:production": "echo \"Flightplan is out of sope for this tut\" && exit 0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.39",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.39",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0-beta.39",
    "@babel/polyfill": "^7.0.0-beta.39",
    "@babel/preset-env": "^7.0.0-beta.39",
    "autoprefixer": "^7.2.5",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^8.0.0-beta.0",
    "classlist-polyfill": "^1.2.0",
    "css-loader": "^0.28.9",
    "css-mqpacker": "^6.0.2",
    "cssnano": "^3.10.0",
    "eslint": "^4.17.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^6.0.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "filemanager-webpack-plugin": "^1.0.22",
    "html-webpack-harddisk-plugin": "^0.1.0",
    "html-webpack-plugin": "^2.30.1",
    "image-webpack-loader": "^4.0.0",
    "lodash": "^4.17.5",
    "node-sass": "^4.7.2",
    "normalize.css": "^7.0.0",
    "postcss-loader": "^2.0.10",
    "resolve-url-loader": "^2.2.1",
    "sass-loader": "^6.0.6",
    "script-ext-html-webpack-plugin": "^1.8.8",
    "style-ext-html-webpack-plugin": "^3.4.7",
    "style-loader": "^0.20.1",
    "stylelint": "^8.4.0",
    "stylelint-config-standard": "^18.0.0",
    "stylelint-scss": "^2.3.0",
    "stylelint-webpack-plugin": "^0.10.1",
    "uglifyjs-webpack-plugin": "^1.1.8",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.10.0",
    "webpack-dev-server": "^2.11.1"
  }
}
